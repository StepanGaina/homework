#include <iostream>
#include <string>

class Corner
{
private:
int x;
public:
Corner(int valueX)
{
x=valueX;
}
void check(int a, int b, int c)
{



if(a+b+c!=x)
printf("This triangle is not exist");
else
printf("This triangle is exist");
}
};
int main()
{
int a, b, c;
scanf("%d",&a);
scanf("%d",&b);
scanf("%d",&c);
Corner x(180);
x.check(a,b,c);

}
